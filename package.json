{
  "name": "astron-lk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:server": "json-server db.json",
    "api:generate": "openapi-zod-client https://astron-lk-open-api.vercel.app/schema.yaml -o ./src/shared/api/v1.ts --export-schemas=true --export-types=true --strict-objects=true --additional-props-default-value=false --implicit-required",
    "postinstall": "pnpm api:generate"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@zodios/core": "^10.9.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "json-server": "1.0.0-beta.3",
    "lucide-react": "^0.469.0",
    "openapi-fetch": "^0.13.3",
    "openapi-react-query": "^0.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tanstack/eslint-plugin-query": "^5.62.9",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "npm-run-all": "^4.1.5",
    "openapi-zod-client": "^1.18.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "zod": "^3.24.1"
  }
}
